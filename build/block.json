{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/woo-products-grid",
  "version": "0.1.0",
  "title": "Woo Products Grid",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
    "html": false
  },
  "textdomain": "woo-products-grid",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "column": {
      "type": "number",
      "default": 3
    },
    "limit": {
      "type": "number",
      "default": 10
    },
    "ACF Field": {
      "type": "string",
      "default": ""
    },
    "showImage": {
      "type": "boolean",
      "default": true
    },
    "showPrice": {
      "type": "boolean",
      "default": true
    },
    "data": {
      "type": "array",
      "default": []
    },
    "acf": {
      "type": "string",
      "default": ""
    }
  }
}